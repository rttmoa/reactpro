/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

view {
  padding: 10rpx; /* 10rpx = 5px     1rpx=0.5px */
  margin: 10rpx;
  background-color: lightskyblue;
}

view:nth-child(1) {
  background-color: lightskyblue;
}